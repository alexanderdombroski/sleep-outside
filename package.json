{
  "name": "sleep-outside-mono",
  "version": "0.6.7",
  "description": "A monorepo for WDD-360 sleep outside project",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd packages/backend && npm run dev",
    "dev:frontend": "cd packages/frontend && npm run dev",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexanderdombroski/sleep-outside.git"
  },
  "keywords": [],
  "author": "Alex Dombroski, Ammon Johnson, Hunter Evans",
  "license": "ISC",
  "type": "module",
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "private": true,
  "homepage": "https://github.com/alexanderdombroski/sleep-outside#readme",
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}

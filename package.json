{
  "name": "sleep-outside-mono",
  "version": "0.6.7",
  "description": "A monorepo for WDD-360 sleep outside project",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"wait-on http://localhost:3000/health && npm run dev:frontend\"",
    "dev:backend": "cd packages/backend && npm run dev",
    "dev:frontend": "cd packages/frontend && npm run dev",
    "build:frontend": "cd packages/frontend && npm run build",
    "lint": "eslint",
    "test": "vitest --watch",
    "test:ci": "vitest run",
    "prepare": "node .husky/prepare.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexanderdombroski/sleep-outside.git"
  },
  "keywords": [],
  "author": "Alex Dombroski, Ammon Johnson, Hunter Evans",
  "license": "ISC",
  "type": "module",
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "private": true,
  "homepage": "https://github.com/alexanderdombroski/sleep-outside#readme",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.3.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.3",
    "eslint-plugin-astro": "^1.6.0",
    "eslint-plugin-svelte": "^3.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.5.0",
    "svelte-eslint-parser": "^1.4.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.4"
  }
}
